<div class="content-body">
  <div class="center">
    <div class="card-container">
      <h1 style="margin-bottom: 2rem">Enroll </h1>
      <form
        class="create-form"
        id="studentForm"
        action="/api/student/create"
        method="POST"
      >
        <%- include('./_form_fields') %>
        <div class="card-footer">
          <a href="/student" class="delete-button">Cancel</a>
          <button
            type="submit"
            class="submit-button"
            id="submitButton"
            disabled
          >
            Submit
          </button>
        </div>
      </form>
    </div>
  </div>
</div>

<script src="/js/email_input.js" defer></script>