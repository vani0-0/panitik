<div class="form-group">
  <label for="name">Name</label>
  <input id="name" type="text" name="name" required autocomplete="additional-name" value="<%= student.name %>"/>
</div>

<div class="form-group">
  <label for="emailInput">Email</label>
  <input
    type="email"
    name="email"
    id="emailInput"
    required
    value="<%= student.email %>"
    autocomplete="email"
  />
</div>
<span id="emailStatus"></span>

  <div class="form-group">
    <label for="grade">Grade Level</label>
    <select id="grade" name="grade" required>
      <option value="7">Grade 7</option>
      <option value="8">Grade 8</option>
      <option value="9">Grade 9</option>
      <option value="10">Grade 10</option>
    </select>
  </div>

  <div class="form-group">
    <label for="gender">Gender</label>
    <select id="gender" name="gender" required>
      <option value="MALE">MALE</option>
      <option value="FEMALE">FEMALE</option>
    </select>
  </div>

<div class="form-group">
  <label for="section" id="section">Section</label>
  <select id="section-select" name="section" required>
  </select>
</div>

<script src="/js/get_sections.js"></script>